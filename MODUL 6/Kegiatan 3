Nama = 'Erbita Putri Apriliyana'
NIM = 'L200240054'
X = '1' + NIM[7]
a = int('1'+ NIM [7:10])
b = len(Nama)
c = 12.5
print(type(a))
print(type(b))
print(a / b)
print(a // b)
print(10 * (a - 999))
print(b ** 2)
print(a % b)
print(type(c))
print(a / c)
print(a // c)
print(a % c)
print(c > b)
print(type(c > b))
print(a > b and b > c)
print(a > 1100 or b < 10)